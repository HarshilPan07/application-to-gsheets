{
    "manifest_version": 3,
    "name": "Hello Extensions Of The World",
    "description": "Base Level Extension",
    "version": "1.0",
    "permissions": [
        "storage",
        "tabs"
    ],
    "action": {
        "default_popup": "hello.html",
        "default_icon": "hello_extensions.png"
    },
    "background": {
        "service_worker": "service_worker.js"
    },
    "content_scripts": [
        {
            "js": [
                "content_scripts.js"
            ],
            "matches": "https://www.linkedin.com/jobs/search/?currentJobId/*"
        }
    ]
}